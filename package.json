{
  "name": "mww",
  "private": true,
  "version": "v5.13.0t",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "docker-login": "docker login -u mww fcr1.fingent.net",
    "docker-build": "docker build -t mww-ui .",
    "docker-compose": "docker-compose up -d --build",
    "docker-tag": "docker tag mww-ui:latest fcr1.fingent.net/mww/mww-ui:%npm_package_version%",
    "docker-list": "docker image ls && docker container ls",
    "docker-prune": "docker container stop mww-front-end_mww_1 && docker container rm mww-front-end_mww_1 && docker image rm mww-ui",
    "docker-push": "docker push fcr1.fingent.net/mww/mww-ui:%npm_package_version%",
    "docker-update": "npm run docker-compose && npm run docker-tag && npm run docker-list && npm run docker-prune && npm run docker-list && npm run docker-push",
    "helm-context": "kubectl.exe config get-contexts && kubectl.exe config use-context fks1.fingent.net",
    "helm-pull": "helm repo update",
    "helm-upgrade": "helm upgrade mww-ui fcr1/mww-ui --install -n mww-test --set image.tag=%npm_package_version% --set ingressroute.enabled=false",
    "helm-pods": "kubectl get pod -n mww-test",
    "helm-update": "npm run helm-context && npm run helm-pull && npm run helm-upgrade && npm run helm-pods"
  },
